<h5> Model Backed Form Helpers </h5>
<%= render '/shared/errors', obj: @post %>
   <%= form_for @post  do |f|  %>

  <%= f.label :title %>
  <%= f.text_field :title %>
  <br/>
 
  <%= f.label :url %>
  <%= f.text_field :url %>
  <br/>

  <%= f.label :description %>
  <%= f.text_field :description %>
  <br/>


  <%= f.submit "Create Post" %>

  <% end  %>