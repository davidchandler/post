<h3>All Posts</h3>

<ul>

<% @posts.each do |post| %>
  <li>
  (
   <%= link_to '+', vote_post_path(post, vote:true),method: 'post' %>
 
   
  <%= post.total_votes.to_s %>

   <%= link_to '-', vote_post_path(post, vote:false),method: 'post' %>
   )
  <%= link_to post.title, post_path(post) %> 
    <%= link_to 'edit', edit_post_path(post) %> </li>
<% end %>

</ul>
